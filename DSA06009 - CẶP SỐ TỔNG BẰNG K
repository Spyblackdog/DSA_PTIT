#include <bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    while (t--){
        int n,s, cnt = 0;
        cin>>n>>s;
        int a[n];
        for (int i=0;i<n;i++)
            cin>>a[i];
        for (int i=0;i<n;i++){
            for (int j=i+1;j<n;j++){
                if (a[i] + a[j] == s)
                    cnt++;
            }
        }
        cout<<cnt<<endl;
    }

    return 0;
}
