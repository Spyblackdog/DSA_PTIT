#include <bits/stdc++.h>

using namespace std;

int n,a[105][105];
vector <string> v;
int check = 0;

void Try(int i,int j,string s){
    if (i == n && j == n && a[i][j] == 1){
        v.push_back(s);
        check = 1;
        return;
    }
    if (i + 1 <= n && a[i+1][j] == 1)
        Try(i+1,j,s + "D");
    if (j + 1 <= n && a[i][j + 1] == 1)
        Try(i,j+1,s + "R");
}

int main(){
    int t;
    cin>>t;
    while (t--){
        cin>>n;
        check = 0;
        for (int i=1;i<=n;i++){
            for (int j=1;j<=n;j++){
                cin>>a[i][j];
            }
        }
        if (a[1][1] == 0){
            cout<<-1<<endl;
            continue;
        }else{
            Try(1,1,"");
            if (check == 0)
                cout<<-1<<endl;
            else{
                for (auto x : v){
                    cout<<x<<" ";
                }
                cout<<endl;
                v.clear();
            }
        }
    }

    return 0;
}
