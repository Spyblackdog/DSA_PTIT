#include <bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		int n,x;
		cin>>n>>x;
		int a[1005];
		pair <pair<int,int>,int> b[n];
		for (int i=0;i<n;i++){
			cin>>a[i];
			b[i] = make_pair(make_pair(abs(x-a[i]),i),a[i]);
		}
		sort(b,b+n);
		for (int i=0;i<n;i++){
			cout<<b[i].second<<" ";
		}
		cout<<endl;
	}
	
	return 0;
}
