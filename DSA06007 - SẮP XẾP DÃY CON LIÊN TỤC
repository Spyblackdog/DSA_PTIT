#include <bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int a[n],b[n];
        for (int i=0;i<n;i++){
            cin>>a[i];
            b[i] = a[i];
        }
        sort(b,b+n);
        int ans1 = INT_MAX,ans2 = INT_MIN;
        for (int i=0;i<n;i++){
            if (a[i] != b[i]){
                if (i < ans1)
                    ans1 = i;
                if (i > ans2)
                    ans2 = i;
            }
        }
        cout<<ans1 + 1<<" "<<ans2 + 1;
        cout<<endl;
    }


    return 0;
}
