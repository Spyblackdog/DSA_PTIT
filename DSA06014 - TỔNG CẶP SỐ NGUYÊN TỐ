#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main(){
    int t;
    cin>>t;
    ll a[1000001] = {0};
    a[0] = a[1] = 1;
    for (ll i=2;i<1000001;i++){
        if (!a[i]){
            for (ll j=i*i;j<1000001;j+=i){
                a[j] = 1;
            }
        }
    }
    while (t--){
        int n;
        cin>>n;
        bool ok = false;
        for (int i=2;i<=n;i++){
            if (a[i] == 0 && a[n - i] == 0){
                cout<<i<<" "<<n-i;
                ok = true;
                break; 
            }
        }
        if  (ok == false)
            cout<<-1;
        cout<<endl;
    }

    return 0;
}
