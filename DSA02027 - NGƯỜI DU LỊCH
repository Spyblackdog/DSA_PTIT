#include <bits/stdc++.h>

using namespace std;

int n, b[16], a[16], c[16][16];
int minc = INT_MAX;

void Try(int i, int sum) {
    if (sum >= minc) return;
    for (int j = 2; j <= n; j++) {
        if (b[j] == 1) {
            a[i] = j;
            b[j] = 0;
            int tempSum = sum + c[a[i-1]][a[i]]; 
            if (i == n) {
                tempSum += c[a[i]][1]; 
                minc = min(tempSum, minc);
            } else {
                Try(i + 1, tempSum); 
            }
            b[j] = 1;  
        }
    }
}

int main() {
    cin >> n;
    for (int i = 1; i <= n; i++) {
        for (int j = 1; j <= n; j++) {
            cin >> c[i][j];
        }
    }
    for (int i = 2; i <= n; i++)
       b[i] = 1;
    a[1] = 1;          
    Try(2, 0); 
    cout << minc << endl;

    return 0;
}
