#include <bits/stdc++.h>

using namespace std;

int a,b;
string s1,s2;
int nho,lon;

void min(void){
	for (int i=0;i<s1.size();i++){
		if (s1[i] == '6'){
			s1[i] = '5';
		}
	}
	for (int i=0;i<s2.size();i++){
		if (s2[i] == '6'){
			s2[i] = '5';
		}
	}
	a = stoi(s1);
	b = stoi(s2);
	nho = a + b;
}

void max(void){
	for (int i=0;i<s1.size();i++){
		if (s1[i] == '5'){
			s1[i] = '6';
		}
	}
	for (int i=0;i<s2.size();i++){
		if (s2[i] == '5'){
			s2[i] = '6';
		}
	}
	a = stoi(s1);
	b = stoi(s2);
	lon = a + b;
}

int main(){
	cin>>a>>b; 
	s1 = to_string(a);
	s2 = to_string(b);
	min();max();
	cout<<nho<<" "<<lon;
	
	return 0;
}
