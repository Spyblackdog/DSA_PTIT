#include <bits/stdc++.h>

using namespace std;

int n,a[105][105],cnt = 0;
vector <string> v;
bool check[105][105];

void Try(int i,int j,string s){
    if (i == n && j == n && a[i][j] == 1){
        v.push_back(s);
        cnt = 1;
        return;
    }
    check[i][j] = true;
    if (i + 1 <= n && a[i + 1][j] == 1 && check[i+1][j] == false)
        Try(i+1,j,s+"D");
    if (i - 1 >= 1 && a[i - 1][j] == 1 && check[i-1][j] == false)
        Try(i-1,j,s+"U");
    if (j + 1 <= n && a[i][j + 1] == 1 && check[i][j+1] == false)
        Try(i,j+1,s + "R");
    if (j - 1 >= 1 && a[i][j - 1 ] == 1 && check[i][j-1] == false)
        Try(i,j-1,s+"L");
    check[i][j] = false;
}

int main(){
    int t;
    cin>>t;
    while (t--){
        cin>>n;
        cnt = 0;
        for (int i=1;i<=n;i++){
            for (int j=1;j<=n;j++){
                cin>>a[i][j];
                check[i][j] = false;
            }
        }
        // if (a[1][1] == 0){
        //     cout<<-1<<endl;
        //     continue;
        // }
        Try(1,1,"");
        if (cnt == 0)
            cout<<-1;
        else{
            sort(v.begin(),v.end());
            for (auto x : v)
                cout<<x<<" ";
            v.clear() ;
        }     
        cout<<endl;
    }

    return 0;
}
