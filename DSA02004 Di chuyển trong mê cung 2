#include <bits/stdc++.h>

using namespace std;

int n,a[105][105];
vector <string> v;
int check = 0;
bool visit[105][105];

void Try(int i,int j,string s){
	if (i == n-1 && j == n-1 && a[i][j] == 1){
		v.push_back(s);
		check = 1;
		return;
	}
	visit[i][j] = true;
	if (i + 1 < n && a[i+1][j] == 1 && visit[i+1][j] == false)
		Try(i+1,j,s+"D");
	if (j + 1 < n && a[i][j+1] == 1 && visit[i][j+1] == false)
		Try(i,j+1,s+"R");
	if (i - 1 >= 0 && a[i-1][j] == 1 && visit[i-1][j] == false)
		Try(i-1,j,s+"U");
	if (j - 1 >= 0 && a[i][j-1] == 1 && visit[i][j-1] == false)
		Try(i,j-1,s+"L"); 
	visit[i][j] = false;
}

int main(){
	int t;
	cin>>t;
	while (t--){
		v.clear();
		cin>>n;
		for (int i=0;i<n;i++){
			for (int j=0;j<n;j++){
				cin>>a[i][j];
				visit[i][j] = false;
			}	
		}
		if (a[0][0] == 1)
			Try(0,0,"");
		if (v.size() == 0)
			cout<<-1<<endl;
		else{
			sort(v.begin(),v.end());
			for (auto x : v)
				cout<<x<<" ";
		}
		cout<<endl;
	}
	
	
	return 0;
}
