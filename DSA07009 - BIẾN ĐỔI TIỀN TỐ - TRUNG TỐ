#include <bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		stack <string> st;
		string s;
		cin>>s;
		for (int i=s.length()-1;i>=0;i--){
			if (s[i] == '+' || s[i] == '-' || s[i] =='/' || s[i] =='*'){
				string s1 = st.top();
				st.pop();
				string s2 = st.top();
				st.pop();
				string temp = "(" + s1 + s[i] + s2 +")";
				st.push(temp);
			}else{
				st.push(string(1,s[i]));
			}
		}
		cout<<st.top()<<endl;
	}
	
	return 0;
}
