#include <bits/stdc++.h>

using namespace std;

vector <int> v[100005];
int a[100005];

int main(){
	int t;
	cin>>t;
	while (t--){
		int n,m;
		cin>>n>>m;
		for (int i=1;i<=m;i++){
			int x,y;
			cin>>x>>y;
			v[x].push_back(y);
		}
		memset(a,0,sizeof(a));
		for (int i=1;i<=n;i++){
			for (auto x : v[i]){
				a[x]++;
			}
		}
		bool ok = true;
		for (int i=1;i<=n;i++){
//			cout<<"Đỉnh thứ "<<i<<" có "<<v[i].size()<<" cầu ra và "<<a[i]<<" cầu vào."<<endl;
			if (a[i] != v[i].size()){
				ok = false;
				break;
			}
		}
		if (!ok){
			cout<<0;
		}else{
			cout<<1;
		}
		cout<<endl;
		for (int i=1;i<=n;i++){
			v[i].clear();
		}
	}
	
	return 0;
}
