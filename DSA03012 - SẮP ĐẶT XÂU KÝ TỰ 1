#include <bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		string s;
		cin>>s;
		int n = s.size();
		map <char,int> m;
		for (int i=0;i<n;i++){
			m[s[i]]++;
		}
		int l = INT_MIN;
		for (auto x : m){
			l = max(x.second,l);
		}
		if (n % 2 == 0){
			if (l <= n - l)
				cout<<1<<endl;
			else
				cout<<-1<<endl;
		}else if (n % 2 != 0){
			if (l <= n - l + 1)
				cout<<1<<endl;
			else
				cout<<-1<<endl;
		}
		m.clear();
	}
	
	return 0;
}
