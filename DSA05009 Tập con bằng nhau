#include <bits/stdc++.h>

using namespace std;

int n,k,a[105],check;
vector <int> v;
vector <vector<int>> vec;
bool flag = true;

void Try(int prev,int sum){
	if (flag == false)	return; 
	for (int j=prev + 1;j<=n;j++){
		if (sum + a[j] <= k){
			v.push_back(a[j]);
			if (sum + a[j] == k){
				vec.push_back(v);
				flag = false;
				return;
			}else{
				Try(j, sum + a[j]);
			}
			v.pop_back();
		}
	}
}

int main(){
	int t;
	cin>>t;
	while (t--){
		vec.clear();
		check = 0;
		cin>>n;
		flag = true;
		for (int i=1;i<=n;i++){
			cin>>a[i];
			check += a[i];
		}
		if (check % 2 == 0){
			k = check / 2;
			sort(a+1,a+n+1);
			Try(0,0);
			sort(vec.begin(),vec.end());
			if (vec.size() == 0)
				cout<<"NO";
			else
				cout<<"YES";
		}else{
			cout<<"NO";
		}
		cout<<endl;
	}
	
	return 0;
}
