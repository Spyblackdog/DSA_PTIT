#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		ll n,s;
		cin>>n>>s;
		ll a[n];
		for (int i=0;i<n;i++)
			cin>>a[i];
		sort(a,a+n);
		ll cnt = 0;
		for (int i=0;i<n-2;i++){
			int j = i + 1;
			int k = n - 1;
			while (j < k){
				ll sum = a[i] + a[j] + a[k];
				if (sum < s){
					cnt += (k - j);
					j++;
				}else{
					k--;
				}
			}
		}
		cout<<cnt<<endl;
	}
	
	return 0;
}
