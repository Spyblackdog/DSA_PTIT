#include <bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		int d;
		cin>>d;
		string s;
		cin>>s;
		int n = s.size();
		map <char,int> m;
		for (int i=0;i<n;i++){
			m[s[i]]++;
		}
		int l = INT_MIN;
		for (auto x : m){
			l = max(x.second,l);
		}
		if ((l-1)*(d-1) <= n - l)
			cout<<1<<endl;
		else
			cout<<-1<<endl;
	}
	
	return 0;
}
