#include <bits/stdc++.h> 

using namespace std;

int n,k,a[10005],b[10005];
vector <vector<int>> v;

void them(int k){
	vector <int> vec;
	for (int i=1;i<=k;i++){
		vec.push_back(a[b[i]]);
	}
	v.push_back(vec);
}

void Try(int i){
	for (int j=b[i-1] + 1;j<=n-k+i;j++){
		b[i] = j;
		if (i == k){
			them(k);
		}else{
			Try(i+1);
		}
	}
}

int main(){
	int t;
	cin>>t;
	while (t--){
		cin>>n>>k;
		v.clear();
		for (int i=1;i<=n;i++){
			cin>>a[i];
		}
		sort(a+1,a+n+1);
			Try(1);
		sort(v.begin(),v.end());
		for (int i=0;i<v.size();i++){
			for (int j=0;j<v[i].size();j++){
				cout<<v[i][j]<<" ";
			}
			cout<<endl;
		}
		cout<<endl;
	}
	
	return 0;
}
