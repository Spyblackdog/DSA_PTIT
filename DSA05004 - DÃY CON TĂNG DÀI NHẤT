#include <bits/stdc++.h>
#define ll long long
const long long mod = 1e9 + 7;

using namespace std;

int main(){
	int n;
	cin>>n;
	int a[n],b[n];
	for (int i=0;i<n;i++){
		cin>>a[i];
		b[i] = 1;
	}
	for (int i=0;i<n;i++){
		for (int j=0;j<i;j++){
			if (a[j] < a[i]){
				b[i] = max(b[i],b[j]+1);
			}
		}
	}
	cout<<*max_element(b,b+n);
	
	return 0;
}
