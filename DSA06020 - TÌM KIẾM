#include <bits/stdc++.h>

using namespace std;

int binary(int n,int a[],int k){
	int l = 0;int r = n - 1;
	while (l <= r){
		int m = (l + r) / 2;
		if (a[m] != k){
			if (a[m] > k){
				r = m - 1; 
			}else{
				l = m + 1;
			}
		}else{
			return m;
		}
	}
	
	return -1;
}

int main(){
	int t;
	cin>>t;
	while (t--){
		int n,k;
		cin>>n>>k;
		int a[n];
		for (int i=0;i<n;i++)
			cin>>a[i];
		if (binary(n,a,k) == -1)
			cout<<-1<<endl;
		else
			cout<<1<<endl;
	}
	
	return 0;
}
