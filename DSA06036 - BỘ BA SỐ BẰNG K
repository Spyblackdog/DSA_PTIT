#include <bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    while (t--){
        int n,s;
        cin>>n>>s;
        int a[n];
        for (int i=0;i<n;i++)
            cin>>a[i];
        sort(a,a+n);
        bool ok = false;
        for (int i=0;i<n;i++){
            int j = i + 1;
            int k = n - 1;
            while (j < k){
                int sum = a[i] + a[j] + a[k];
                if (sum == s){
                    ok = true;
                    break;
                }else if (sum < s){
                    j++;
                }else{
                    k--;
                }
            }
        }
        if (ok == false)
            cout<<"NO"<<endl;
        else
            cout<<"YES"<<endl;
    }

    return 0;
}
