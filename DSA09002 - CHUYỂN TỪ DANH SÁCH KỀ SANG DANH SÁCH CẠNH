#include <bits/stdc++.h>

using namespace std;

int main(){
	int n;
	cin>>n;
	cin.ignore();
	int a[n+1][n+1];
	memset(a,0,sizeof(a));
	for (int i=1;i<=n;i++){
		string s,num;
		getline(cin,s);
		stringstream ss(s);
		while (ss >> num){
			a[i][stoi(num)] = 1;
			a[stoi(num)][i] = 1;
		}
	}
	for (int i=1;i<=n;i++){
		for (int j=1;j<=n;j++){
			if (a[i][j] == 1){
				cout<<i<<" "<<j<<endl;
				a[i][j] = a[j][i] = 0;
			}
		}
	}
	
	return 0;
}
