#include <bits/stdc++.h>

using namespace std;

int main(){
int t;
cin>>t;
while (t--){
    int n,m;
    cin>>n>>m;
    set <int> s1,s2;
    int a[n],b[m],c[100005] = {0};
    for (int i=0;i<n;i++){
        cin>>a[i];
        c[a[i]] = 1;
        s1.insert(a[i]);
    }
    for (int j=0;j<m;j++){
        cin>>b[j];
        s1.insert(b[j]);
        if (c[b[j]] == 1){
            s2.insert(b[j]);
        }
    }
    for (auto x : s1)
        cout<<x<<" ";
    cout<<endl;
    for (auto v : s2)
        cout<<v<<" ";
    cout<<endl;
}

return 0;
}
