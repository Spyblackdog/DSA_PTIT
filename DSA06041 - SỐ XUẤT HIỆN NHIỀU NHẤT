#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int a[n];
        map <int,int> m;
        for (int i=0;i<n;i++){
            cin>>a[i];
            m[a[i]]++;
        }
        int ma = INT_MIN;
        int ans = INT_MAX;
        for (auto x : m){
            if (ma < x.second){
                ma = x.second;
                ans = x.first;
            }
        }
        if (ma > n/2)
            cout<<ans;
        else   
            cout<<"NO";
        cout<<endl;
    }

    return 0;
}
