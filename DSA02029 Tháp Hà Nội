#include <bits/stdc++.h>

using namespace std;

int n;

void chuyen(int n,char a,char b){
	cout<<a<<" -> "<<b<<endl;
}

void thap(int n,char c1,char c2,char c3){
	if (n == 1)
		chuyen(n,c1,c3);
	else{
		thap(n-1,c1,c3,c2);
		chuyen(n,c1,c3);
		thap(n-1,c2,c1,c3);
	}
}

int main(){
	cin>>n;
	thap(n,'A','B','C');
	
	return 0;
}
