#include <bits/stdc++.h> 

using namespace std;

int n,a[10005],b[10005];
vector <vector<int>> v;

int prime(int n){
	if (n < 2)
		return 0;
	else{
		for (int i=2;i<=sqrt(n);i++){
			if (n % i == 0){
				return 0;
			}
		}
	}
	
	return 1;
}

int check(int k){
	int sum = 0;
	for (int i=1;i<=k;i++){
		sum += a[b[i]];
	}
	if (prime(sum) == 1)
		return 1;
	
	return 0;
}

void them(int k){
	vector <int> vec;
	for (int i=1;i<=k;i++){
		vec.push_back(a[b[i]]);
	}
	v.push_back(vec);
}

void Try(int i,int k){
	for (int j=b[i-1] + 1;j<=n-k+i;j++){
		b[i] = j;
		if (i == k){
			if (check(k)){
				them(k);
			}
		}else{
			Try(i+1,k);
		}
	}
}

int main(){
	int t;
	cin>>t;
	while (t--){
		cin>>n;
		v.clear();
		for (int i=1;i<=n;i++){
			cin>>a[i];
		}
		sort(a+1,a+n+1);
		reverse(a+1,a+n+1);
		for (int k=1;k<=n;k++){
			Try(1,k);
		}
		sort(v.begin(),v.end());
		for (int i=0;i<v.size();i++){
			for (int j=0;j<v[i].size();j++){
				cout<<v[i][j]<<" ";
			}
			cout<<endl;
		}
		cout<<endl;
	}
	
	return 0;
}
