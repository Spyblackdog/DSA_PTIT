#include <bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    while (t--){
        int n,k;
        cin>>n>>k;
        int a[n],c[100005] = {0};
        for (int i=0;i<n;i++){
            cin>>a[i];
            c[a[i]]++;
        }
        if (c[k] == 0)
            cout<<-1<<endl;
        else
            cout<<c[k]<<endl;
    }


}
