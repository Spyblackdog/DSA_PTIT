#include <bits/stdc++.h>

using namespace std;

vector <int> v[100005];
bool ok[100005];

void dfs(int u){
	cout<<u<<" ";
	ok[u] = true;
	for (auto x : v[u]){
		if (!ok[x]){
			dfs(x);
		}
	}
}

int main(){
	int t;
	cin>>t;
	while (t--){
		int n,m,k;
		cin>>n>>m>>k;
		for (int i=1;i<=m;i++){
			int x,y;
			cin>>x>>y;
			v[x].push_back(y);
			v[y].push_back(x);
		}
		memset(ok,false,sizeof(ok));
		dfs(k);
		cout<<endl;
		for (int i=1;i<=n;i++)
			v[i].clear();
	}
	
	return 0;
}
