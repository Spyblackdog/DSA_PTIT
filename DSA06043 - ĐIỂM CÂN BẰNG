#include <bits/stdc++.h>

using namespace std;

int main(){
    int t;
    cin>>t;
    while (t--){
        int n;
        cin>>n;
        int a[n];
        int sum = 0;
        for (int i=0;i<n;i++){
            cin>>a[i];
            sum += a[i];
        }
        bool ok = false;
        int sl = 0;
        for (int i=0;i<n;i++){
            sum -= a[i];
            if (sum == sl){
                ok = true;
                cout<<i+1<<endl;
                break;
            }
            sl += a[i];
        }
        if (ok == false)
            cout<<-1<<endl;
    }

    return 0;
}
