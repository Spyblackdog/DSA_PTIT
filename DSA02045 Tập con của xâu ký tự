#include <bits/stdc++.h>

using namespace std;

int n,k,a[10005],b[1005];
string s;
vector <vector<int>> v;

void them(int k){
	vector <int> vec;
	for (int i=1;i<=k;i++){
		vec.push_back(a[b[i]]);
	}
	v.push_back(vec);
}


void Try(int i,int k){
	for (int j = b[i - 1] + 1;j<= n - k + i;j++){
		b[i] = j;
		if (i == k)
			them(k);
		else
			Try(i+1,k);
	}
}

int main(){
	int t;
	cin>>t;
	while (t--){
		cin>>n;
		cin>>s;
		v.clear();
		for (int i=0;i<n;i++){
			a[i + 1] = s[i] - 'a' + 1; 
		}
		for (int k=1;k<=n;k++){
			Try(1,k);
		}
		sort(v.begin(),v.end());
		for (int i=0;i<v.size();i++){
			for (int j=0;j<v[i].size();j++){
				cout<<char(v[i][j] + 96);
			}
			cout<<" ";
		}
		cout<<endl;
	}
	
	
	return 0;
}
