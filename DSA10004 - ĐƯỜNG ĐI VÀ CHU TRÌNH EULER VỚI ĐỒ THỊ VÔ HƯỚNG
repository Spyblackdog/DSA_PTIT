#include <bits/stdc++.h>

using namespace std;

vector <int> v[10005];

int main(){
	int t;
	cin>>t;
	while (t--){
		int n,m;
		cin>>n>>m;
		for (int i=1;i<=m;i++){
			int x,y;
			cin>>x>>y;
			v[x].push_back(y);
			v[y].push_back(x);
		}
		int c = 0,l = 0;
		for (int i=1;i<=n;i++){
			if (v[i].size() % 2 == 0)
				c++;
			else
				l++; 
		}
		if (c == n)
			cout<<2;
		else if (l == 2)
			cout<<1;
		else
			cout<<0;
		cout<<endl;
		for (int i=1;i<=n;i++)
			v[i].clear();
	}
	
	
	return 0;
}
