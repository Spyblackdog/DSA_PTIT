#include <bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		int n;
		cin>>n;
		int a[100005];
		for (int i=0;i<n;i++)
			cin>>a[i];
		sort(a,a+n);
		string s1,s2;
		int cnt = 0;
		if (n % 2 == 0){
			for (int i=0;i<n;i+=2){
				s1 += to_string(a[i]);
				s2 += to_string(a[i+1]);
			}
		}else{
			for (int i=0;i<n;i+=2){
				s1 += to_string(a[i]);
				if (i != n-1)
					s2 += to_string(a[i+1]);
			}
		}
		long long c = stoll(s1);
		long long d = stoll(s2);
		cout<<c+d<<endl;
	}
	
	return 0;
}
