#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main(){
	int n;
	cin>>n;
	int a[n];
	for (int i=0;i<n;i++)
		cin>>a[i];
	for (int i=0;i < n - 1;i++){
		int mi = i;
		for (int j = i + 1;j < n;j++){
			if (a[mi] > a[j])
				mi = j;
		}
		swap(a[mi],a[i]);
		cout<<"Buoc "<<i + 1<<": ";
		for (int k=0;k<n;k++){
			cout<<a[k]<<" ";
		}
		cout<<endl;
	}	
	
	return 0;
}
