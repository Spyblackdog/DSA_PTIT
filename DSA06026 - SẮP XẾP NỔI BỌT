#include <bits/stdc++.h>

using namespace std;

int main(){
    int n;
    cin>>n;
    int a[n];
    for (int i=0;i<n;i++)
        cin>>a[i];
    int tt = 1;
    for (int i=0;i<n-1;i++){
        int k = 0;
        for (int j=0;j<n-i-1;j++){
            if (a[j] > a[j + 1]){
                swap(a[j],a[j+1]);
                k++;
            }
        }
        if (k != 0){
            cout<<"Buoc "<<tt<<": ";
            for (int l=0;l<n;l++)
                cout<<a[l]<<" ";
            tt++;
            cout<<endl;
        }
    }

    return 0;
}
