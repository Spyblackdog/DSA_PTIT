#include <bits/stdc++.h>

using namespace std;

int n,k,a[1005],cnt = 0;
vector <vector<int>> vec;
vector <int> v;

void Try(int pre,int sum){
	for (int j = pre;j<=n;j++){
		if (sum + a[j] <= k){
			v.push_back(a[j]);
			if (sum + a[j] == k){
				vec.push_back(v);
				cnt++;
			}else{
				Try(j,sum + a[j]);
			}
			v.pop_back();
		}
	}
}

int main(){
	int t;
	cin>>t;
	while (t--){
		cin>>n>>k;
		cnt = 0;
		for (int i=1;i<=n;i++){
			cin>>a[i];
		}
		Try(1,0);
		if (vec.size() == 0)
			cout<<-1<<endl;
		else{
			cout<<cnt<<" ";
			for (auto x : vec){
				cout<<"{";
				for (int i=0;i<x.size();i++){
					cout<<x[i];
					if (i != x.size() - 1)
					cout<<" ";
				}
				cout<<"} ";
			}
			vec.clear();
		}
		cout<<endl;
	}
	
	return 0;
}
