#include <bits/stdc++.h>

using namespace std;

int n,s,a[10005],cnt = 0,ans = INT_MAX,ok = 0;

void  Try(int pre,int cnt,int s){
	for (int j=pre+1;j<=n;j++){
		if (a[j] > s)
			continue;
		if (a[j] == s){
			ans = min(ans,cnt+1);
			ok = 1;
		}else if (cnt + 1 < ans){
			Try(j,cnt+1,s - a[j]);
		}
	}
}

int main(){
	cin>>n>>s;
	for (int i=1;i<=n;i++)
		cin>>a[i];
	sort(a+1,a+n+1,greater<int>());
	Try(0,0,s);
	if (ok == 0)
		cout<<-1;
	else
		cout<<ans;
	
	return 0;
}
