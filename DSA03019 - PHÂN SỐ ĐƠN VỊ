#include <bits/stdc++.h>
#define ll long long

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		ll a,b;
		cin>>a>>b;
		int temp = a;
		while (a != 0){
			if (a != temp && a != 0){
				cout<<" + ";
			}
			ll n = ceil((double)b/a);
			cout<<"1/"<<n;
			a = a * n - b;
			b = b * n;
		}
		cout<<endl;
	}
	
	return 0;
}
