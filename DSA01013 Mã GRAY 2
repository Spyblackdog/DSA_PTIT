#include <bits/stdc++.h>

using namespace std;

int main(){
	int t;
	cin>>t;
	while (t--){
		string s;
		cin>>s;
		int n = s.size();
		int a[10005],b[10005];
		for (int i=0;i<n;i++)
			a[i] = s[i] - '0';
		b[0] = a[0];
		for (int i=1;i<n;i++){
			if (a[i] != b[i - 1])
				b[i] = 1;
			else
				b[i] = 0;
		}
		for (int i=0;i<n;i++)
			cout<<b[i];
		cout<<endl;
	}
	
	return 0;
}
